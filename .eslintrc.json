{
    "extends": ["prettier", "airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "env": {
        "commonjs": true,
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
        "no-console": 0,
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 100,
                "tabWidth": 4,
                "semi": true
            }
        ]
    },
    "plugins": ["prettier"]
}
